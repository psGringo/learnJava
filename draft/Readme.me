start backend
start ui
start nginx


backend:
  ./gradlew clean
  ./gradlew build
  ./gradlew run // will run on 8080 port

ui:
  npm run build:dev | npm run build
  npm run serve // will run on 2077 port

nginx:
  ./nginx/kill       //will kill all nginx processes on windows

  # nginx start on windows from powershell
  cmd
  cd nginx
  nginx.exe

  ||
  cmd
  cd nginx
  start nginx.exe