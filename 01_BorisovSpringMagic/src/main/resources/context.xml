<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

            <context:annotation-config/>
            <context:component-scan base-package="com.stanley.springMagic"/>

    <bean class="com.stanley.springMagic.quaters.PostProxyInvokerContextListener"></bean>
    <bean class="com.stanley.springMagic.quaters.ProfilingHandlerBeanPostProcessor"></bean>
    <bean class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor"></bean>
    <bean class="com.stanley.springMagic.quaters.InjectRandomIntAnnotationBeanPostProcessor"></bean>
    <bean class="com.stanley.springMagic.quaters.TerminatorQuater" id="terminatorQuoter">
        <property name="message" value="i'll be back"/>
    </bean>

</beans>